{
  "name": "minedive",
  "version": "0.0.4",
  "description": "Minedive is a privacy conscious P2P search engine",
  "permissions": [
    "idle",
    "*://*.google.com/",
    "*://*.google.it/",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "images/minedive16.png",
    "32": "images/minedive32.png",
    "48": "images/minedive48.png",
    "128": "images/minedive128.png"
  },
  "background": {
    "scripts": [
      "vendor/nacl-util.min.js",
      "vendor/nacl-fast.min.js",
      "conf.js",
      "browserAction.js",
      "lib.js",
      "l1.js",
      "l2.js",
      "histbook.js",
      "debug.js",
      "background_opts.js",
      "db.js",
      "search_engine/google.js",
      "webrtc.js",
      "websock.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/minedive16.png",
      "32": "images/minedive32.png",
      "48": "images/minedive48.png",
      "128": "images/minedive128.png"
    }
  },
  "manifest_version": 2
}
