{
  "name": "minedive",
  "version": "0.0.5",
  "description": "Minedive is a privacy conscious P2P search engine",
  "permissions": [
    "idle",
    "*://*.google.com/",
    "*://*.google.it/",
    "*://arxiv.org/",
    "*://*.arxiv.org/",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "images/minedive16.png",
    "32": "images/minedive32.png",
    "48": "images/minedive48.png",
    "128": "images/minedive128.png"
  },
  "background": {
    "scripts": [
      "vendor/nacl-util.min.js",
      "vendor/nacl-fast.min.js",
      "search_engine/google.js",
      "search_engine/arxiv.js",
      "core/circuit.js",
      "core/conf.js",
      "core/browserAction.js",
      "core/lib.js",
      "core/l1.js",
      "core/l2.js",
      "core/histbook.js",
      "core/debug.js",
      "core/background_opts.js",
      "core/webrtc.js",
      "core/websock.js",
      "core/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "ui/popup.html",
    "default_icon": {
      "16": "images/minedive16.png",
      "32": "images/minedive32.png",
      "48": "images/minedive48.png",
      "128": "images/minedive128.png"
    }
  },
  "manifest_version": 2
}
